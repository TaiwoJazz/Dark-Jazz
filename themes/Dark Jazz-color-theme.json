			],
			"settings": {
				"foreground": "#C3E88D",
				"fontStyle": ""
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#09E85E",
				"fontStyle": ""
			}
		},
		{
			"name": "Entity Types",
