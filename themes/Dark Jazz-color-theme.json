{
	"name": "Dark Jazz",
	"colors": {
		// coding area
		"editor.background": "#0d0c2b",
		"editor.wordHighlightBackground": "#fdfcff40",
		"editor.lineHighlightBorder": "#353c56",
		"editorLineNumber.foreground": "#6f7d97",
		"editorLineNumber.activeForeground": "#09E85E",
		"editorGroupHeader.tabsBackground": "#0d0c2b",
		"editor.hoverHighlightBackground": "#05098fb5",
		// tooltip when over on codes
		"editorHoverWidget.background": "#0d0c2b",
		"editorHoverWidget.border": "#5064ac",

		// title bar at the top
		"titleBar.activeBackground": "#0d0c2b",
		"titleBar.inactiveBackground": "#0d0c2b",
		"titleBar.activeForeground": "#e3e5ff",

		// code folder background
		"sideBar.background": "#0d0c2b",
		"sideBarSectionHeader.background": "#0d0c2b",

		//Project folder name at top left
		"sideBarSectionHeader.foreground": "#444b67",
		"sideBarSectionHeader.border": "#353c56", 
		"sideBarTitle.foreground": "#727a96",

		// vs code main icons color & background
		"activityBar.foreground": "#6f7d97",
		"activityBar.activeBackground": "#0d0c2b",
		"activityBar.background": "#0d0c2b",
		"activityBarBadge.background": "#007acc",

		// bottom stauts menu
		"statusBar.background": "#0d0c2b",
		"statusBar.foreground": "#8898b6",
		"statusBar.border": "#353c56",
		"statusBar.noFolderBackground" : "#0d0c2b",
    "statusBar.debuggingBackground": "#0d0c2b",

		// all folders arrow icons 
		"icon.foreground": "#6f7d97",

		// main settings (Files, Edit, Selection)
		"menu.background": "#070448",
		"menu.foreground": "#b4c2dc",
		"menu.selectionBackground": "#05098fb5",
		"menu.separatorBackground": "#124eae",
		"menubar.selectionForeground": "#6f7d97",
		
		// toggle between code tab to another at top
		"tab.activeBackground": "#121143",
		"tab.activeBorder": "#0184ff",
		"tab.inactiveBackground": "#0d0c2b",

		// folders backgrounds and hover
		"list.activeSelectionBackground": "#05035a", 
		"list.activeSelectionForeground": "#ffffff",
		"list.focusOutline": "#0d0c2b",
		"list.activeSelectionIconForeground": "#0084ff",
		"list.hoverBackground": "#121143",
		"list.inactiveSelectionBackground": "#05035a",
		
		// inside input box
		"input.background": "#070448",
		"input.foreground": "#e9e8ff",
		"input.placeholderForeground": "#868dab",
		"quickInput.background": "#070448",
		"quickInput.foreground": "#b4c2dc",

		// folders color when git is initialize
		"gitDecoration.untrackedResourceForeground": "#3ddf7b",
		"gitDecoration.modifiedResourceForeground": "#98bd60",
		"gitDecoration.ignoredResourceForeground": "#546E7A",
		
		"tree.indentGuidesStroke": "#444b67",

		"extensionButton.background": "#0d0c2b",

		"welcomePage.background": "#0d0c2b",
		
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#546E7A"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#21e1b4",
				"fontStyle": ""
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#C792EA"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				// "punctuation",
				// "meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				// "punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#5ab5da",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#73d4f7",
				"fontStyle": "italic"
				
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#35c5fa"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
